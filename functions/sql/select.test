select '*' from foo
union
select 'distinct *' from foo


select 'foo.*, bar.b' from 'foo as f' join 'bar on foo.a = bar.a'
union
select 'foo.*, bar.b' from foo full join bar on 'foo.a = bar.a and foo.b = bar.b'
union
select 'foo.*, bar.b' from foo left join bar on 'foo.a = bar.a' where "foo.a > 1 and foo.b = 'b'"

select 'count(*)' from foo
union
select 'count(distinct a)' from foo
union
select 'min(a)' from foo
union
select 'max(a)' from foo
union
select 'sum(a)' from foo
union
select 'avg(a)' from foo

select 'a, count(*)' from foo group by a
order by a asc
union all
select 'a, count(*)' from foo group by a having 'count(*) > 1'
order by a desc

